{
    "name": "zoilomora/factusol-rest-api",
    "description": "The REST-API to implement B2B in FactuSOL.",
    "version": "dev-master",
    "type": "project",
    "keywords": ["laravel", "factusol", "rest-api", "api", "b2b"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zoilo Mora",
            "email": "zoilo.mora@hotmail.com",
            "homepage": "http://zoilomora.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "v5.4.30",
        "laravel/tinker": "v1.0.2",
        "oomphinc/composer-installers-extender": "v1.1.2",
        "doctrine/dbal": "v2.5.12",

        "rap2hpoutre/laravel-log-viewer": "v0.10.2",
        "davejamesmiller/laravel-breadcrumbs": "3.0.3",

        "almasaeed2010/adminlte": "v2.3.11",
        "fortawesome/font-awesome": "v4.7.0",
        "driftyco/ionicons": "v2.0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "optimize-autoloader": true
    },
    "extra": {
        "installer-types": [
            "library",
            "component"
        ],
        "installer-paths": {
            "public/assets/{$name}/": [
                "almasaeed2010/adminlte",
                "fortawesome/font-awesome",
                "driftyco/ionicons"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/zoilomora/factusol-rest-api/issues"
    }
}
